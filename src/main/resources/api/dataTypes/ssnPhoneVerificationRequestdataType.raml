#%RAML 1.0 DataType
properties:
  SSN:
    type : string
    description: Social security number
    required: true
    example: "051-12-2022"
    pattern: '\d{3}-\d{2}-\d{4}$'
  PhoneNumber:
    type : string
    description: Phone number
    required: true
    pattern: '\d{3}-\d{3}-\d{4}$'
    example: "123-123-1234"